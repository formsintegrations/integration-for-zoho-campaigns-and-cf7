  "scripts": {
    "lint": "eslint --ext .jsx --ext .js frontend/src/ --fix",
    "hot": "vite --force",
    "build": "vite build && npm run i18n:php",
    "production": "composer i --no-dev && pnpm install && pnpm build",
    "clear-lint-cache": "eslint . --cache --cache-file eslint-cache/eslintcache",
    "i18n:json": "npx po2json locale.pot ../languages/e_zc-admin-script.json -f jed1.x",
    "i18n:php": "[ ! -d \"languages/\" ] && mkdir languages/;node ./pot-to-php.js locale.pot languages/generatedString.php i18nStrings;wp i18n make-pot ./ --include=*.php --skip-js --slug=fitzocacf --headers='{\"Last-Translator\":\"support@formsintegrations.com\",\"Language-Team\":\"support@formsintegrations.com\",\"PO-Revision-Date\":\"\",\"Report-Msgid-Bugs-To\":\"\"}'",
    "clean": "npx rimraf node_modules yarn.lock package-lock.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "**/*.js?(x)": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie 11",
      "ie 10",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "ie 10",
      "ie > 9",
      "ie 9-11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
